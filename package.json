{
  "name": "playwright2gotest",
  "version": "1.0.0",
  "description": "Convert Playwright tests to Go tests via HAR files",
  "scripts": {
    "test": "playwright test",
    "test:debug": "PWDEBUG=1 playwright test",
    "test:ui": "playwright test --ui",
    "test:crowdstrike": "playwright test crowdstrike.spec.ts",
    "test:httpbin": "playwright test httpbin.spec.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
} 